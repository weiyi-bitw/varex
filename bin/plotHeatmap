#!/usr/bin/env python

import sys
import plotly.plotly as py
from plotly.graph_objs import *


def main():
    py.sign_in("mdsfdwei1", "1218eamhn2")
    
    fi = open("test.out", "rb")
    l = fi.readline()
    samples = l.strip().split()[1:]
    variants = []
    aac = []

    for l in fi:
        tokens = l.strip().split('\t')
        variants.append(tokens[0])
        aac.append([int(x) for x in tokens[1:]])

    data = Data([
        Heatmap(
            z = aac,
            x = samples,
            y = variants
            )
        ])
    
    plot_url = py.plot(data, filename='UK10K_CHD_gene_high_impact')

if __name__ == "__main__":
    sys.exit(main());


